#running=`ps auxww | grep -v grep | grep '\.\/tss_server' | awk '{print $2}'`
#running=`ps auxww | grep -v grep | grep '/usr/bin/ruby /home/skip/mmt/REPOS/ao/trunk/bin/tss_server' | awk '{print $2}'`
running=`ps auxww | grep -v grep | grep "/usr/bin/perl -w ${PCR_HOME}/bin/tss_server" | awk '{print $2}'`
if [ $running ]; then 
    echo "1"
else
    echo "0"
fi
